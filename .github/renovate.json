{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "dependencyDashboardAutoclose": true,
  "enabledManagers": ["pip-compile", "pip_setup"],
  "pip-compile": {
    "fileMatch": ["(^|/)requirements\\.in$"],
    "ignoreDeps": ["colorama"],
    "schedule": ["at 8:00 am every weekday"]
  }
}
